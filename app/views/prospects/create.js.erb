<% if @prospect.errors.any? %>
	$('#errors').html("<%= escape_javascript(render 'msg') %>")
	$('span').hide();
<% else %> 
	$('#new_prospect')[0].reset();
	$('#errors').hide();
	$('span').hide();
	$('#noticeMsg').text('Successfully created');
	$('#table').html("<%= escape_javascript(render 'prospects')%>");
<% end %>