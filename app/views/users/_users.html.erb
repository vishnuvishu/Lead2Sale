<table id="table">
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td class="editThis"><%= user.full_name %></td>
        <td class="editThis"><%= user.email %></td>
        <td class="editThis"><%= user.phone %></td>
        <td class="editThis"><%= user.role_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
